{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "dist": true,
        "lib": true,
        "docs": true,
        "wiki": true,
        "release": true,
        "_templates": true,
        "src/js/Resources": true,
        "build/bundle": true,
        "gulpfile.babel.js": true,
        ".npmignore": true,
        ".gitignore": true,
        "manual-conf": true,
        ".github": true,
        "package-lock.json": true,
        "templates/assets-*": true,
        "templates/root-*": true,
    },
    "files.associations": {
        "**/webparts/**/*.txt": "xml",
        "*.webpart": "xml"
    },
    "editor.formatOnSave": false,
    "html.format.unformatted": "a, abbr, acronym, b, bdo, big, br, button, cite, code, dfn, em, i, img, input, kbd, label, map, object, q, samp, script, select, small, span, strong, sub, sup, textarea, tt, var, Value, %@",
    "powershell.developer.editorServicesLogLevel": "None"
}