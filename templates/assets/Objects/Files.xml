<pnp:Files xmlns:pnp="http://schemas.dev.office.com/PnP/2018/01/ProvisioningSchema">
    <pnp:File Src="sitetemplates\FullTemplate.txt" Folder="SiteTemplates" Overwrite="true">
        <pnp:Properties>
            <pnp:Property Key="GtOrder" Value="10" />
            <pnp:Property Key="GtIsEnabled" Value="1" />
            <pnp:Property Key="Title" Value="{resource:SiteTemplates_FullTemplate_Title}" />
            <pnp:Property Key="Comments" Value="{resource:SiteTemplates_FullTemplate_Comments}" />
            <pnp:Property Key="GtIsDefault" Value="1" />
        </pnp:Properties>
    </pnp:File>
    <pnp:File Src="sitetemplates\LiteTemplate.txt" Folder="SiteTemplates" Overwrite="true">
        <pnp:Properties>
            <pnp:Property Key="GtOrder" Value="20" />
            <pnp:Property Key="GtIsEnabled" Value="1" />
            <pnp:Property Key="Title" Value="{resource:SiteTemplates_LiteTemplate_Title}" />
            <pnp:Property Key="Comments" Value="{resource:SiteTemplates_LiteTemplate_Comments}" />
            <pnp:Property Key="GtIsDefault" Value="0" />
        </pnp:Properties>
    </pnp:File>
    <pnp:Directory Src="js\" Folder="siteassets/pp/js" Overwrite="true" Recursive="false" IncludedExtensions="*.js,*.map" />
    <pnp:Directory Src="css\" Folder="siteassets/pp/css" Overwrite="true" Recursive="false" IncludedExtensions="*.css" />
    <pnp:Directory Src="img\" Folder="siteassets/pp/img" Overwrite="true" Recursive="false" IncludedExtensions="*.png" />
    <pnp:Directory Src="config" Folder="siteassets/pp/config" Overwrite="true" Recursive="false" IncludedExtensions="*.txt" />
</pnp:Files>